<h1 id = "header">Team Member{{#if multiple}}s{{/if}}</h1>

<ul>
  {{#each members}}
  <h3>{{this.fname}} {{this.lname}} - {{this.year}}</h3>

      <img src="/img/{{this.user}}.png" width="100" height="100" />
      <p>{{this.desc}}</p>

  {{/each}}
</ul>

<head>
    <link rel="stylesheet" type="text/css" href="/stylesheets/teampage.css">
</head>


