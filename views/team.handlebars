<!-- Main section-->
<section style="margin-left: 0">
    <!-- Page content-->
    <div class="content-wrapper">
        <div class="row">
            <div class="col-xs-12">
                <h1 id = "header">Team Member{{#if multiple}}s{{/if}}</h1>

                <ul>
                    {{#each members}}
                        <h3>{{this.fname}} {{this.lname}} - {{this.year}}</h3>

                        <img src="/img/{{this.user}}.png" width="100" height="100" />
                        <p>{{this.desc}}</p>

                    {{/each}}
                </ul>
            </div>
        </div>
    </div>
</section>
<!-- Page footer-->
<footer style="margin-left: 0">
    <span>Copyright &copy; 2015 Classwire</span>
</footer>